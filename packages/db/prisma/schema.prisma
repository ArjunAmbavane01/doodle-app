generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         String   @id @default(uuid())
  name       String   @db.VarChar(191)
  email      String   @unique @db.VarChar(191)
  provider   String   @db.VarChar(100)
  photo      String?
  oauth_id   String
  created_at DateTime @default(now())

  @@map("users")
}
